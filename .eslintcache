lt='avatar' style={{width:'50%'}}/>\r\n            <div className='container'>\r\n         <h1>Welcome {admin.username}</h1>\r\n         <p>Email-{admin.email}</p>\r\n         <p>Enterprise Name- {admin.businessName}</p>\r\n         <p>address- {admin.address}</p>\r\n         <p>Total customers- {customers.length}</p>\r\n         <p>Total Products- {products.length}</p>\r\n         <p>Total Bill Amount- {sum} Rs</p>\r\n         </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Admin","C:\\Users\\bsrinivas3\\Desktop\\Javascript\\Bill-app-project\\Bill-App-Application\\bill-app\\src\\Action\\adminAction.js",["197"],"import axios from '../Config/axios'\r\nimport {Redirect} from 'react-router-dom'\r\nimport swal from 'sweetalert'\r\n\r\nconst token= JSON.parse(localStorage.getItem('token'))\r\n\r\nconst adminAccount=(result)=> {\r\n    return {\r\n        type: 'LIST_ADMIN',\r\n        payload: result\r\n    }\r\n}\r\n\r\nexport const startGetAccount=()=> {\r\n    return (dispatch)=> {\r\n\r\n        axios.get('/users/account', {\r\n            headers : {\r\n                'Authorization': `Bearer ${token}`\r\n            }\r\n        })\r\n        .then(res=>{\r\n             const result= res.data\r\n            //  console.log('account', result)\r\n             \r\n             if(result) {\r\n                dispatch(adminAccount(result))\r\n             } else {\r\n                 swal(\"Sorry\", \"You need to login first\")\r\n             }\r\n            \r\n            \r\n        })\r\n        .catch(err=> {\r\n            alert(err.message)\r\n        })\r\n    }\r\n}","C:\\Users\\bsrinivas3\\Desktop\\Javascript\\Bill-app-project\\Bill-App-Application\\bill-app\\src\\Reducers\\adminReducer.js",["198"],"import React from 'react'\r\n\r\nconst initialValue={}\r\nconst adminReducer=(state=initialValue, action)=> {\r\n    switch(action.type) {\r\n\r\n        case 'LIST_ADMIN' : {\r\n            // console.log('admin reducer',action.payload)\r\n      